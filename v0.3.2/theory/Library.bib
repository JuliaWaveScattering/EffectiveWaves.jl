	shortjournal = {Opt. Express, {OE}},
	author = {Yi, Ji and Radosevich, Andrew J. and Rogers, Jeremy D. and Norris, Sam C. P. and Çapoğlu, İlker R. and Taflove, Allen and Backman, Vadim},
