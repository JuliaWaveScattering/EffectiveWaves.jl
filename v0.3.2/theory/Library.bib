	author = {Liu, I.-Shih},
	urldate = {2016-05-06},
	date = {2007-11-07},
	langid = {english},
	keywords = {Mechanics, Automotive and Aerospace Engineering, Traffic, General entropy inequality, Thermodynamics with Lagrange multiplier, Entropy flux relation, 74A20, 74B20, 80A17},
	file = {Full Text PDF:/home/art/uom/References/Zotero/storage/MDQ792W4/Liu - 2007 - Entropy Flux Relation for Viscoelastic Bodies.pdf:application/pdf;Snapshot:/home/art/uom/References/Zotero/storage/7SC5C7DP/s10659-007-9142-0.html:text/html}
