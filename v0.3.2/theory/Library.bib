	file = {ACM Full Text PDF:/home/art/uom/References/Zotero/storage/4X45PW4K/Ganesh and Hawkins - 2017 - Algorithm 975 TMATROM—A T-Matrix Reduced Order Mo.pdf:application/pdf}
}

